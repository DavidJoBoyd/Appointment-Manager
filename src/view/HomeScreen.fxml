<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomeScreen">
   <children>
      <Label fx:id="homeLabel" layoutX="284.0" layoutY="38.0" text="Home" />
      <Button fx:id="appButton" layoutX="200.0" layoutY="118.0" mnemonicParsing="false" onAction="#toAppointments" prefHeight="27.0" prefWidth="200.0" text="View Appointments" />
      <Button fx:id="custButton" layoutX="200.0" layoutY="173.0" mnemonicParsing="false" onAction="#toCustomers" prefHeight="26.0" prefWidth="200.0" text="View Customers" />
      <Button fx:id="reportButton" layoutX="200.0" layoutY="222.0" mnemonicParsing="false" onAction="#toReports" prefHeight="26.0" prefWidth="200.0" text="View Reports" />
      <Button fx:id="logoutButton" layoutX="273.0" layoutY="314.0" mnemonicParsing="false" onAction="#toLogout" text="Logout" />
      <Label fx:id="message" alignment="CENTER" contentDisplay="CENTER" layoutX="227.0" layoutY="72.0" text="No Upcoming Appointments" textAlignment="CENTER" />
   </children>
</AnchorPane>
