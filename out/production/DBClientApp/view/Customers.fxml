<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Customers">
   <children>
      <Label layoutX="60.0" layoutY="59.0" text="Customers" />
      <TableView fx:id="custTable" layoutX="30.0" layoutY="118.0" prefHeight="401.0" prefWidth="950.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="Customer ID" />
          <TableColumn fx:id="nameCol" prefWidth="107.19998168945312" text="Customer Name" />
            <TableColumn fx:id="phoneCol" prefWidth="112.19996643066406" text="Phone Number" />
            <TableColumn fx:id="stateCol" prefWidth="99.0" text="State / Province" />
            <TableColumn fx:id="postCol" prefWidth="74.5999755859375" text="Postal Code" />
            <TableColumn fx:id="addressCol" prefWidth="108.39996337890625" text="Address" />
            <TableColumn fx:id="createDateCol" prefWidth="92.20001220703125" text="Created Date " />
            <TableColumn fx:id="createByCol" prefWidth="77.199951171875" text="Created By" />
            <TableColumn fx:id="updateCol" prefWidth="80.800048828125" text="Last Update" />
            <TableColumn fx:id="updateColBy" prefWidth="124.2000732421875" text="Last Updated By" />
        </columns>
      </TableView>
      <Button fx:id="modifyButton" layoutX="478.0" layoutY="542.0" mnemonicParsing="false" onAction="#update" text="Modify" />
      <Button fx:id="addButton" layoutX="409.0" layoutY="542.0" mnemonicParsing="false" onAction="#add" text="Add" />
      <Button fx:id="deleteButton" layoutX="551.0" layoutY="542.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
      <Button fx:id="backButton" layoutX="909.0" layoutY="542.0" mnemonicParsing="false" onAction="#toMain" text="Back" />
   </children>
</AnchorPane>
